<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Seating comfort</th>
      <th>Ac</th>
      <th>Movie audio quality</th>
      <th>Movie video output</th>
      <th>Suggestion</th>
      <th>Complaint</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.seating_comfort %></td>
        <td><%= feedback.ac %></td>
        <td><%= feedback.movie_audio_quality %></td>
        <td><%= feedback.movie_video_output %></td>
        <td><%= feedback.suggestion %></td>
        <td><%= feedback.complaint %></td>
        <td><%= feedback.user_id %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
